<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="SSH-root" namespace="/" extends="struts-default">
		<action name="login" method="{1}" class="AllAction">
			<result name="success">/NormalUserPages/NormalUser.jsp</result>
		</action>
		<action name="regist" method="{1}" class="AllAction">
			<interceptor-ref name="tokenStack"></interceptor-ref>
			<result name="success">/NormalUserPages/NormalUser.jsp</result>
		</action>
	</package>
</struts>